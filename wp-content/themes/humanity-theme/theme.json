        "line-height": "33px",
        "font-weight": "bold",
        "font-family": "var(--wp--preset--font-family--secondary)"
      },
      "heading-fourth": {
        "font-size": "26px",
        "line-height": "29px",
        "font-weight": "bold",
        "font-family": "var(--wp--preset--font-family--secondary)"
