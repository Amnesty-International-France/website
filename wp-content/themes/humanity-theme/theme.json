            {
              "slug": "white",
              "color": "#fff",
              "name": "White"
            },
            {
              "slug": "amnesty-grey_brand",
              "color": "#b3b3b3",
              "name": "Grey Brand"
            }
          ]
        },
        "typography": {
          "fluid": false,
          "fontSizes": [
            {
              "slug": "small",
