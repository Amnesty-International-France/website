        "typography": {
          "fontSize": "var(--wp--custom--link--font-size)",
          "lineHeight": "var(--wp--custom--link--line-height)",
          "fontWeight": "var(--wp--custom--link--font-weight)",
          "fontFamily": "var(--wp--custom--link--font-family)"
        },
        ":hover": {
          "color": {
            "background": "var(--wp--preset--color--primary)"
          }
        }
      }
    },
    "spacing": {
      "padding": {
        "top": "0px",
        "right": "2rem",
        "bottom": "0px",
        "left": "2rem"
      }
    }
