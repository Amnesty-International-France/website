          "name": "Black"
        },
        {
          "slug": "white",
          "color": "#fff",
          "name": "White"
        },
        {
          "slug": "primary",
          "color": "#ffff00",
          "name": "Primary"
        },
        {
          "slug": "primary-state",
          "color": "#f8d72b",
          "name": "Primary State"
        },
        {
          "slug": "secondary",
          "color": "#2196f3",
          "name": "Secondary"
        },
        {
          "slug": "accent",
          "color": "#2196f3",
          "name": "Accent"
        },
        {
          "slug": "grey-lightest",
