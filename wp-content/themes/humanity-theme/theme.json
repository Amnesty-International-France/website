      "body": {
        "font-size": "18px",
        "line-height": "28px",
        "font-weight": "lighter",
        "font-family": "var(--wp--preset--font-family--primary)"
      },
      "body-bold": {
        "font-size": "var(--wp--custom--body--font-size)",
        "line-height": "var(--wp--custom--body--line-height)",
        "font-weight": "bold",
        "font-family": "var(--wp--custom--body--font-family)"
      },
      "body-secondary": {
        "font-size": "16px",
        "line-height": "24px",
        "font-weight": "lighter",
        "font-family": "var(--wp--preset--font-family--primary)"
      },
      "body-secondary-bold": {
